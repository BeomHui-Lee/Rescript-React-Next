{
    "name": "rescript-react-next",
    "version": "0.1.0",
    "license": "MIT",
    "repository": "https://github.com/BeomHui-Lee/Rescript-React-Next",
    "private": false,
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "dev": "next",
        "debug": "NODE_OPTIONS='--inspect' next",
        "build": "rescript && next build",
        "now-build": "rescript && next build",
        "export": "next export",
        "start": "next start -p $PORT",
        "res:build": "rescript",
        "res:clean": "rescript clean",
        "res:start": "rescript build -w",
        "css:build": "tailwindcss -i ./styles/main.css -o styles/.tailwind.dev.css --minify",
        "css:start": "tailwindcss -i ./styles/main.css -o styles/.tailwind.dev.css --watch",
        "dev:watch": "yarn css:build && concurrently yarn:css:start yarn:res:start yarn:dev"
    },
    "devDependencies": {
        "@greenlabs/res-tailwindcss": "^0.1.6",
        "@rescript/react": "^0.10.3",
        "@tailwindcss/custom-forms": "^0.2.1",
        "autoprefixer": "10.1.0",
        "chalk": "^4.1.0",
        "concurrently": "^6.2.1",
        "copy-webpack-plugin": "^7.0.0",
        "cssnano": "5.0.5",
        "etag": "^1.8.1",
        "express": "^4.17.1",
        "gentype": "4.1",
        "html-webpack-plugin": "^5.2.0",
        "mime": "^2.5.2",
        "next-transpile-modules": "7.1.2",
        "postcss": "8.2.15",
        "postcss-cli": "8.3.1",
        "raw-loader": "^4.0.2",
        "rescript": "^9.1.4",
        "rescript-devserver-tools": "^1.0.9",
        "tailwindcss": "^2.2.15",
        "webpack": "^5.24.2",
        "webpack-cli": "^4.5.0"
    },
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@rescript/react": "^0.10.3",
        "bs-fetch": "^0.6.2",
        "next": "10.2.3",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-helmet": "^6.1.0",
        "rescript-asyncdata": "^2.0.0",
        "rescript-future": "^2.0.0",
        "rescript-js": "^0.5.6",
        "rescript-request": "^3.0.1",
        "rescript-test": "^3.0.0",
        "swr": "^1.0.1"
    }
}